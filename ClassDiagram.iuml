@startuml

class Salle {
    -String batiment
    -int numero
    -int capacite
    -String intitule
    -String infos
}

class Cours {
    -Enum TD/TP/CM/CC/CT
}

class Personne {
    -String nom
    -String prenom
    -Date dateDeNaissance
    -String email
}

class Promotion {
    -String intitule
}

class Module {
    -int nbHeures
}

class Contrainte {
    -Date dateDeb
    -Date dateFin
}

class Groupe {
    -String numero
}

class Etudiant

class Enseignant {
    -String laboratoire
    -Enum MC/Prof/Vacataire
}

class Reservation {
    -Date dateDeb
    -Date dateFin
    -String intitule
    -String memo
    -Enum NP/Annule/Reporte
}

class Jury

class Concours 

Salle "1" -- "*" Reservation

Etudiant "*" --* "*" Groupe

Groupe "*" --* "1" Promotion 

Cours "*" ---- "*" Groupe

Cours "*" -- "*" Module

Enseignant "1" -- "*" Materiel

Enseignant "1" -- "*" Contrainte

Cours -up-|> Reservation

Soutenance -up-|> Reservation

Jury -up-|> Reservation

Concours -up-|> Reservation

Etudiant --|> Personne

Enseignant --|> Personne

Concours "*" -- "*" Etudiant

Reservation "*" -- "*" Enseignant

Jury "1" -- "1" Promotion

Soutenance "*" -- "1" Etudiant

@enduml
